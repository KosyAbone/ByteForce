<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".KickAshApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KickAsh"
        tools:targetApi="31">
        <activity
            android:name="com.byteforce.kickash.ui.login.LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.KickAsh"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name="com.byteforce.kickash.ui.main.SlideShowActivity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire1Activity"         android:theme="@style/Theme.KickAsh"
            />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire2Activity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire9Activity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire8Activity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire7Activity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire6Activity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire5Activity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire4Activity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.Questionnaire3Activity" />
        <activity android:name="com.byteforce.kickash.ui.main.articles.ArticlesDetailActivity" />
        <activity android:name="com.byteforce.kickash.ui.questionair.QuestionnaireEndActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
    </application>

</manifest>